{{> navbar}}

<h3>Current Employees:</h3>
<table id="employees">
    <thead> 
        <th>Employee ID</th> 
        <th>First Name</th> 
        <th>Last Name</th> 
        <th>Email</th>
    </thead> 
    <tbody>
        {{#each employees}} 
        <tr>
            <td>{{this.userID}}</td> 
            <td>{{this.firstName}}</td> 
            <td>{{this.lastName}}</td> 
            <td>{{this.email}}</td> 
        </tr> 
        {{/each}} 
    </tbody> 
</table> 